void sort(int *a, int l, int r) {
    if (l >= r){
        return;
    }

    int m = (l + r) / 2;
    sort(a, l, m);
    sort(a, m + 1, r);

    int i = l;
    int j = m + 1;

    int uk = l;
    while (i <= m && j <= r)
        if (a[i] < a[j]){
            b[uk] = a[i];
            i = i + 1;
        } else {
            b[uk] = a[j];
            j = j + 1;
        }

    for (int k = i; k <= m; k = k + 1) {
        b[uk] = a[k];
        uk = uk + 1;
    }
    for (int k = j; j <= r; k = k + 1){
        b[uk] = a[k];
        uk = uk + 1;
    }
    for (int i = l; i <= r; i = i + 1)
        a[i] = b[i];

}


