#include<io>

void swap(int* a, int* b){
    int t = *a;
    *a = *b;
    *b = t;
}

int b[1000];
int a[1000];

void sort(int l, int r) {
    if (l >= r){
        return;
    }

    int m = (l + r) / 2;
    sort(l, m, a, b);
    sort(m + 1, r, a, b);

    int i = l;
    int j = m + 1;

    int uk = l;
    while (i <= m && j <= r)
        if (*(a + i * 8) < *(a + j * 8)){
            *(b + uk * 8) = *(a + i * 8);
            i = i + 1;
        } else {
           *(b + uk * 8) = *(a + j * 8);
            j = j + 1;
        }

    for (int k = i; k <= m; k = k + 1) {
        *(b + uk * 8) = *(a + k * 8);
        uk = uk + 1;
    }
    for (k = j; j <= r; k = k + 1){
        *(b + uk * 8) = *(a + k * 8);
        uk = uk + 1;
    }
    for (i = l; i <= r; i = i + 1)
        *(a + i * 8) = *(b + i * 8);
     return;
}


void read_array(int len, int* a){
    for (int i = 0; i < len; i = i + 1)
