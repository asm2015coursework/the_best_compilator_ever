int write(int fd, char* buf, int count) {
    asm {
mov rax, 1
mov rdi, qword[rbp + 8]
mov rsi, qword[rbp + 16]
mov rdx, qword[rbp + 24]
syscall
    }
    return;
}

int read(int fd, char* buf, int count) {
    asm {
mov rax, 0
mov rdi, qword[rbp + 8]
mov rsi, qword[rbp + 16]
mov rdx, qword[rbp + 24]
syscall
    }
    return;
}

